{
  "expo": {
    "name": "DaHora Filmes",
    "slug": "dahora-filmes",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/dahora.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splash-dahora.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-dahora.png",
        "backgroundColor": "#000000"
      },
      "package": "com.melissajl.dahorafilmes"
    },
    "plugins": [
      "expo-font",
      "expo-router"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "1fde6c4f-bb9e-44aa-9036-5d3961b1ad75"
      }
    },
    "owner": "melissajl"
  }
}
